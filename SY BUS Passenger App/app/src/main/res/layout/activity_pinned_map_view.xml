    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.sybus.android.userapp.ui.pinned_map_view">

    <include
        android:id="@+id/app_bar"
        layout="@layout/view_app_bar" />


    <View
        android:id="@+id/pinned_map_view_tabbar_bottom_line"
        android:layout_below="@id/app_bar"
        android:layout_width="wrap_content"
        android:layout_height="3dp"
        android:background="@drawable/tab_bar_shadow"/>

    <FrameLayout
        android:id="@+id/pinnedMapView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/pinned_map_view_tabbar_bottom_line">

