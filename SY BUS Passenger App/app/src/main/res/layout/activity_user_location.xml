<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.sybus.android.userapp.ui.UserLocationActivity">

    <include
        android:id="@+id/app_bar"
        layout="@layout/view_app_bar" />


    <View
        android:id="@+id/user_location_activity_tabbar_bottom_line"
        android:layout_below="@id/app_bar"
        android:layout_width="wrap_content"
        android:layout_height="3dp"
        android:background="@drawable/tab_bar_shadow"/>


<FrameLayout
    android:id="@+id/userLocationMapView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_below="@id/user_location_activity_tabbar_bottom_line">

